  ]},
// [0,1] 默认0.5

  {"name":"LCAS","effects":[
    {"effect":"LCAS","scale":[-1,-1],"sharpness":0.33,"threshold":0.2},
  ]},
// [0,1] 默认0.33 ｜ [0,1] 默认0.2

  {"name":"LumaSharpen","effects":[
    {"effect":"LumaSharpen","sharpStrength":0.65,"sharpClamp":0.035,"pattern":1,"offsetBias":1},
  ]},
// [1e-5,+∞] 默认0.65 ｜ [0,1] 默认0.035 ｜ [0,3] 取整 默认1 ｜ [0,+∞] 默认1

  {"name":"最邻近","effects":[
    {"effect":"Nearest","scale":[-1,-1]},
  ]},
