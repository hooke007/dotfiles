  {"name":"双三次","effects":[
    {"effect":"Bicubic","scale":[-1,-1],"paramB":0.333333,"paramC":0.333333},
  ]},
// [0,1] 默认0.333333 ｜ [0,1] 默认0.333333

  {"name":"双线性","effects":[
    {"effect":"Bilinear","scale":[-1,-1]},
  ]},

  {"name":"对比度自适应锐化","effects":[
    {"effect":"CAS","sharpness":0.4},
  ]},
// [0,1] 默认 0.4

  {"name":"对比度自适应锐化-带缩放版","effects":[
    {"effect":"CAS_Scaling","scale":[-1,-1],"sharpness":0.4},
  ]},
